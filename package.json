{
  "name": "josh-collinsworth-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "original-dev": "vite dev --host --port 1027",
    "sass:watch": "sass src/lib/assets/scss/global.scss src/lib/assets/scss/global.css --style=compressed --watch",
    "sass:build": "sass src/lib/assets/scss/global.scss static/global.css --style=compressed",
    "dev": "npm-run-all --parallel sass:watch original-dev",
    "host": "vite dev --port 1027 --host",
    "build": "npm run sass:build && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "postbuild": "svelte-sitemap --domain https://joshcollinsworth.com"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "mdsvex": "^0.11.2",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "sass": "^1.38.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.0",
    "svelte-sitemap": "^2.1.1",
    "tslib": "^2.0.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0"
  },
  "type": "module",
  "dependencies": {
    "just-debounce": "^1.1.0",
    "just-throttle": "^4.2.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-slug": "^5.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
